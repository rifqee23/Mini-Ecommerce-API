  "paths": {
    "/auth/register": {
      "post": {
        "tags": ["Auth API"],
        "description": "This endpoint allows a new user to register an account by providing required information",
        "summary": "Register new user",

        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string"
                  },
                  "role": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Success create user",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponseCreate"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["Auth API"],
        "description": "his endpoint authenticates a user using their credentials and returns an access token and a refresh token if valid",
        "summary": "User Login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success Login",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponseLogin"
                }
              }
            }
          }
        }
      }
    },
    "/auth/refresh": {
      "post": {
        "tags": ["Auth API"],
        "description": "Refresh token is sent automatically via HttpOnly cookie.",
        "summary": "refresh token",
        "parameters": [
          {
            "in": "cookie",
            "name": "refresh_token",
            "schema": {
              "type": "string"
            },
            "required": true
